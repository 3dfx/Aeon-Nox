  <include name="StripshowVideoInfo">
    <control type="group">
      <description>Video Info</description>
      <posx>0</posx>
      <posy>990</posy>
      <animation effect="slide" start="0" end="150" reversible="true" condition="Skin.HasSetting(nostudioratingflags)">Conditional</animation>
      <control type="group">
        <description>Episode Info</description>
        <visible>Container.Content(episodes)</visible>
        <control type="label">
          <posx>1740</posx>
          <posy>0</posy>
          <height>57</height>
          <width>930</width>
          <font>Font_Reg17</font>
          <textcolor>white</textcolor>
          <aligny>center</aligny>
          <align>right</align>
          <label>$INFO[ListItem.Label,[COLOR abFFFFFF][/COLOR]]</label>
        </control>
        <control type="label">
          <posx>1740</posx>
          <posy>30</posy>
          <height>57</height>
          <width>930</width>
          <font>Font_Reg17</font>
          <textcolor>abFFFFFF</textcolor>
          <aligny>center</aligny>
          <align>right</align>
          <label>$INFO[ListItem.Rating]$INFO[ListItem.Duration, | , $LOCALIZE[12391] ]$INFO[ListItem.Premiered, | ]</label>
          <visible>!Skin.HasSetting(InfoPopUpEpisode)</visible>
        </control>
        <control type="label">
          <posx>1740</posx>
          <posy>30</posy>
          <height>57</height>
          <width>930</width>
          <font>Font_Reg17</font>
          <textcolor>abFFFFFF</textcolor>
          <aligny>center</aligny>
          <align>right</align>
          <label>$INFO[ListItem.TVShowTitle]</label>
          <visible>Skin.HasSetting(InfoPopUpEpisode)</visible>
        </control>
      </control>
      <control type="group">
        <description>Episode Info</description>
        <visible>Container.Content(movies)</visible>
        <control type="label">
          <posx>1740</posx>
          <posy>0</posy>
          <height>57</height>
          <width>930</width>
          <font>Font_Reg17</font>
          <textcolor>white</textcolor>
          <aligny>center</aligny>
          <align>right</align>
          <label>$INFO[ListItem.Label,[COLOR abFFFFFF][/COLOR]]</label>
        </control>
        <control type="label">
          <posx>1740</posx>
          <posy>33</posy>
          <height>57</height>
          <width>930</width>
          <font>Font_Reg17</font>
          <textcolor>abFFFFFF</textcolor>
          <aligny>center</aligny>
          <align>right</align>
          <label>$INFO[ListItem.Year]$INFO[ListItem.Duration, | , $LOCALIZE[12391] ]$INFO[ListItem.RatingAndVotes, | ]$INFO[ListItem.Top250, | Top 250 #]</label>
