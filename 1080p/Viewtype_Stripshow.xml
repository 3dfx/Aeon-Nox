          <label>$INFO[ListItem.Tagline]</label>
          <visible>Skin.HasSetting(InfoPopUp)</visible>
          <scroll>true</scroll>
        </control>
      </control>
    </control>
  </include>
  <include name="StripshowTVThumbsVars">
    <posx>-8.</posx>
    <posy>-6</posy>
    <width>256</width>
    <height>158</height>
    <bordersize>14</bordersize>
    <aspectratio scalediffuse="false">scale</aspectratio>
  </include>
  <include name="StripshowTVThumbs">
    <control type="group">
      <visible>Container.Content(episodes)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]</visible>
      <posx>-75</posx>
      <control type="image">
        <include>StripshowTVThumbsVars</include>
        <texture diffuse="thumbs/multiplex_tvmask.png">DefaultEpisode.png</texture>
        <bordertexture border="14">thumbs/multiplex_tvborder.png</bordertexture>
        <info>ListItem.Icon</info>
      </control>
      <control type="group">
        <animation effect="fade" start="100" end="0" time="400" easing="out" tween="cubic" reversible="true">Focus</animation>
        <visible>Container.Content(episodes)</visible>
        <control type="image">
          <posx>4.</posx>
          <posy>87</posy>
          <width>230</width>
          <height>51</height>
          <texture>thumbs/multiwall_label.png</texture>
        </control>
        <control type="label">
          <posx>118</posx>
          <posy>106</posy>
          <width>216</width>
          <align>center</align>
          <font>Font_Reg14</font>
          <label>$INFO[ListItem.Season,$LOCALIZE[20359] ]$INFO[ListItem.Episode,.]</label>
        </control>
      </control>
      <control type="image">
        <include>StripshowTVThumbsVars</include>
        <texture diffuse="thumbs/multiplex_tvmask.png">thumbs/multiplex_tv_glass.png</texture>
      </control>
      <control type="image">
        <posx>189.</posx>
        <posy>8</posy>
        <width>45</width>
        <height>45</height>
        <include condition="!Skin.HasSetting(AltNewOverlay)">OverlayVisibility0</include>
        <include condition="Skin.HasSetting(AltNewOverlay)">AltOverlayVisibility0</include>
      </control>
    </control>
  </include>
  <include name="StripshowPosterThumbs">
    <control type="group">
      <visible>Container.Content(movies)</visible>
      <animation effect="slide" start="0,0" end="45,-82" time="0" tween="cubic" easing="in" reversible="true" condition="Container.Content(movies)">Conditional</animation>
      <control type="image">
        <posx>48</posx>
        <posy>4</posy>
        <width>146</width>
        <height>213</height>
        <texture background="true" diffuse="wall/wall_movie_mask.png">DefaultVideoBigPoster.png</texture>
        <aspectratio scalediffuse="false">scale</aspectratio>
        <info>ListItem.Icon</info>
        <fadetime>500</fadetime>
      </control>
      <control type="image">
        <posx>48</posx>
        <posy>4</posy>
        <width>146</width>
        <height>213</height>
        <texture>wall/wall_nfoverlay.png</texture>
        <bordertexture border="-6">wall/wall_movie_nfborder.png</bordertexture>
      </control>
      <control type="image">
        <posx>148</posx>
        <posy>4</posy>
        <width>45</width>
        <height>45</height>
        <include condition="!Skin.HasSetting(AltNewOverlay)">OverlayVisibility0</include>
        <include condition="Skin.HasSetting(AltNewOverlay)">AltOverlayVisibility0</include>
      </control>
    </control>
  </include>
  <include name="VideoListContent">
    <posx>-45</posx>
    <posy>840</posy>
    <width>2100</width>
    <height>225</height>
    <focusposition>4</focusposition>
    <visible>Container.Content(episodes)|Container.Content(movies)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]</visible>
    <itemlayout width="240" height="225" condition="Container.Content(episodes)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]">
      <include>StripshowTVThumbs</include>
    </itemlayout>
    <focusedlayout width="240" height="225" condition="Container.Content(episodes)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]">
      <control type="group">
        <animation effect="zoom" center="30,50" start="100" end="155" time="400" easing="out" tween="cubic" reversible="false">Focus</animation>
        <animation effect="zoom" center="30,50" start="155" end="100" time="200" easing="out" tween="cubic" reversible="false">UnFocus</animation>
        <include>StripshowTVThumbs</include>
      </control>
    </focusedlayout>
    <itemlayout width="210" height="225" condition="Container.Content(movies)">
      <include>StripshowPosterThumbs</include>
    </itemlayout>
    <focusedlayout width="210" height="225" condition="Container.Content(movies)">
      <control type="group">
        <animation effect="zoom" center="110,16" start="100" end="155" time="200" easing="out" tween="cubic" reversible="false">Focus</animation>
        <animation effect="zoom" center="110,16" start="155" end="100" time="200" easing="out" tween="cubic" reversible="false">UnFocus</animation>
        <include>StripshowPosterThumbs</include>
      </control>
    </focusedlayout>
  </include>
  <include name="Stripshow">
    <control type="group">
      <visible>Control.IsVisible(517)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <include>Animation_HideStripShow</include>
      <control type="scrollbar" id="60">
        <posx>0</posx>
        <posy>992</posy>
        <width>1920</width>
        <height>88</height>
        <animation effect="fade" start="100" end="0" time="400" condition="Control.HasFocus(517)|ControlGroup(9000).HasFocus|Control.HasFocus(7001)">Conditional</animation>
        <include>Animation_HideStripShow</include>
        <texturesliderbackground>osd/osd_progress_clear.png</texturesliderbackground>
        <texturesliderbar>osd/osd_progress_clear.png</texturesliderbar>
        <texturesliderbarfocus>osd/osd_progressmid.png</texturesliderbarfocus>
        <textureslidernib>views/scrollBarNib.png</textureslidernib>
        <textureslidernibfocus>views/scrollBarNib.png</textureslidernibfocus>
        <pulseonselect>yes</pulseonselect>
        <onup>517</onup>
        <ondown condition="Window.IsActive(pictures)">195</ondown>
        <ondown condition="!Window.IsActive(pictures)">517</ondown>
        <showonepage>false</showonepage>
        <orientation>horizontal</orientation>
      </control>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>1920</width>
        <height>1080</height>
        <texture>views/stripshow_background.png</texture>
        <visible>[Window.IsActive(pictures)+!Skin.HasSetting(StripshowWS)]|Container.Content(movies)</visible>
      </control>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>1920</width>
        <height>1080</height>
        <texture>views/stripshow_background_tvshows.png</texture>
        <visible>Container.Content(episodes)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]</visible>
      </control>
      <include condition="Window.isActive(videolibrary)">StripshowVideoInfo</include>
      <include condition="Window.IsActive(pictures)">PictureInfo</include>
      <control type="wraplist" id="517">
        <viewtype label="31380">bigwrap</viewtype>
        <pagecontrol>60</pagecontrol>
        <scrolltime tween="quadratic" easing="out">200</scrolltime>
        <onleft>517</onleft>
        <onright>517</onright>
        <onup condition="!IsEmpty(ListItem.Trailer)">7001</onup>
        <onup condition="IsEmpty(ListItem.Trailer)">9000</onup>
        <ondown>60</ondown>
        <orientation>horizontal</orientation>
        <include condition="Window.IsActive(pictures)+!Skin.HasSetting(StripshowWS)">PictureListContent</include>
        <include condition="Window.IsActive(videolibrary)|[Window.IsActive(pictures)+Skin.HasSetting(StripshowWS)]">VideoListContent</include>
      </control>
      <control type="button" id="195">
        <include>HiddenObject</include>
        <onleft>517</onleft>
        <onright>517</onright>
        <onup>517</onup>
        <ondown>517</ondown>
        <onclick condition="Window.IsActive(pictures)">SlideShow($INFO[ListItem.Path],notrandom)</onclick>
      </control>
    </control>
  </include>
</includes>