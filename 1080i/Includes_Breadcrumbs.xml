        <lefttexture>dialogs/progress/progress_left.png</lefttexture>
        <midtexture>dialogs/progress/progress_mid.png</midtexture>
        <righttexture>dialogs/progress/progress_right.png</righttexture>
        <overlaytexture>-</overlaytexture>
        <info>Player.ProgressCache</info>
      </control>
      <control type="progress" id="20">
        <description>Progressbar</description>
        <posx>525</posx>
        <posy>1</posy>
        <width>180</width>
        <height>20</height>
        <colordiffuse>$INFO[Skin.String(diffuse_color)]</colordiffuse>
        <texturebg>osd/seek/progresscache_back.png</texturebg>
        <lefttexture>dialogs/progress/progress_left.png</lefttexture>
        <midtexture>dialogs/progress/progress_mid.png</midtexture>
        <righttexture>dialogs/progress/progress_right.png</righttexture>
        <overlaytexture>-</overlaytexture>
        <info>Player.Progress</info>
      </control>
      <control type="fadelabel">
        <posx>142</posx>
        <posy>4</posy>
        <width>360</width>
        <height>27</height>
        <font>Font_Reg17</font>
        <textcolor>white2</textcolor>
        <scroll>true</scroll>
        <label>[COLOR a8FFFFFF]$INFO[MusicPlayer.Artist][/COLOR]</label>
        <label>[COLOR white2]$INFO[MusicPlayer.Title][/COLOR]</label>
        <scrollout>false</scrollout>
        <scrollspeed>50</scrollspeed>
        <pauseatend>10000</pauseatend>
        <visible>Player.HasAudio</visible>
      </control>
      <control type="fadelabel">
        <posx>142</posx>
        <posy>4</posy>
        <width>360</width>
        <height>27</height>
        <font>Font_Reg17</font>
        <textcolor>white2</textcolor>
        <scroll>true</scroll>
        <label>[COLOR a8FFFFFF]$INFO[Videoplayer.TVShowTitle][/COLOR]</label>
        <label>[COLOR white2]$INFO[Videoplayer.Title][/COLOR]</label>
        <scrollout>false</scrollout>
        <scrollspeed>50</scrollspeed>
        <pauseatend>10000</pauseatend>
        <visible>!IsEmpty(Videoplayer.TVShowTitle) + Player.HasVideo</visible>
      </control>
      <control type="fadelabel">
        <posx>142</posx>
        <posy>4</posy>
        <width>360</width>
        <height>27</height>
        <font>Font_Reg17</font>
        <textcolor>white2</textcolor>
        <scroll>true</scroll>
        <label>$INFO[Videoplayer.Title]</label>
        <scrollout>false</scrollout>
        <scrollspeed>50</scrollspeed>
        <pauseatend>10000</pauseatend>
        <visible>IsEmpty(Videoplayer.TVShowTitle) + Player.HasVideo</visible>
      </control>
      <control type="label">
        <description>Current time</description>
        <posx>465</posx>
        <posy>15</posy>
        <width>300</width>
        <height>30</height>
        <font>Font_Reg14</font>
        <label>$VAR[HomeOSDLabelVar]</label>
        <align>center</align>
        <textcolor>white2</textcolor>
      </control>
    </control>
  </include>
  <include name="Breadcrumbs_Furniture">
    <control type="image" id="4051">
      <texture>-</texture>
      <visible>![Container.Scrolling|Container.OnScrollNext|Container.OnScrollPrevious]</visible>
      <animation effect="fade" start="0" end="100" time="0">Visible</animation>
      <animation effect="fade" time="500">Hidden</animation>
    </control>
    <control type="group">
      <!--clock widget -->
      <posx>748r</posx>
      <posy>0</posy>
      <visible>Skin.HasSetting(HasBackbar)</visible>
      <animation type="WindowClose" reversible="true" condition="!Window.Next(1111) + !Window.IsActive(1111) + !Window.Next(10012) + !Window.IsActive(10012) + !Window.Next(10013) + !Window.IsActive(10013) + !Window.Next(10014) + !Window.IsActive(10014) + !Window.Next(10015) + !Window.IsActive(10015) + !Window.Next(10016) + !Window.IsActive(10016) + !Window.Next(10017) + !Window.IsActive(10017) + !Window.Next(10018) + !Window.IsActive(10018) + !Window.Next(10019) + !Window.IsActive(10019) + !Window.Next(10007) + !Window.IsActive(10007)">
        <effect type="slide" start="0,0" end="0,-52" time="300" tween="cubic" easing="in" />
      </animation>
      <animation type="WindowOpen" reversible="true" condition="!Window.Previous(settings) + !Window.Previous(1111) + !Window.Previous(10012) + !Window.Previous(10013) + !Window.Previous(10014) + !Window.Previous(10015) + !Window.Previous(10016) + !Window.Previous(10017) + !Window.Previous(10018) + !Window.Previous(10019) + !Window.Previous(10007)">
        <effect type="slide" start="0,-52" end="0,0" time="400" tween="cubic" easing="in" />
      </animation>
      <animation effect="slide" start="750" end="0" time="300">VisibleChange</animation>
      <control type="image">
        <description>time background</description>
        <posx>150</posx>
        <posy>0</posy>
        <width>794</width>
        <height>87</height>
        <animation effect="slide" start="0" end="200" time="400" condition="!Skin.HasSetting(breadcrumbsweather)">Conditional</animation>
        <texture flipy="true">home/timepanel_mid.png</texture>
        <colordiffuse>$INFO[Skin.String(diffuse_home)]</colordiffuse>
        <include>Animation_VisibleChange400</include>
      </control>
    </control>
    <control type="group">
      <!--left bg -->
      <posx>0</posx>
      <posy>0</posy>
      <visible>Skin.HasSetting(HasBackbar)</visible>
      <animation type="WindowClose" reversible="true" condition="!Window.Next(1111) + !Window.IsActive(1111) + !Window.Next(10012) + !Window.IsActive(10012) + !Window.Next(10013) + !Window.IsActive(10013) + !Window.Next(10014) + !Window.IsActive(10014) + !Window.Next(10015) + !Window.IsActive(10015) + !Window.Next(10016) + !Window.IsActive(10016) + !Window.Next(10017) + !Window.IsActive(10017) + !Window.Next(10018) + !Window.IsActive(10018) + !Window.Next(10019) + !Window.IsActive(10019) + !Window.Next(10007) + !Window.IsActive(10007)">
        <effect type="slide" start="0,0" end="0,-52" time="300" tween="cubic" easing="in" />
      </animation>
      <animation type="WindowOpen" reversible="true" condition="!Window.Previous(settings) + !Window.Previous(1111) + !Window.Previous(10012) + !Window.Previous(10013) + !Window.Previous(10014) + !Window.Previous(10015) + !Window.Previous(10016) + !Window.Previous(10017) + !Window.Previous(10018) + !Window.Previous(10019) + !Window.Previous(10007)">
        <effect type="slide" start="0,-52" end="0,0" time="400" tween="cubic" easing="in" />
      </animation>
      <animation effect="slide" start="-750" end="0" time="300">VisibleChange</animation>
      <control type="image">
        <description>time background</description>
        <posx>-150</posx>
        <posy>0</posy>
        <width>794</width>
        <height>87</height>
        <texture flipy="true" flipx="true">home/timepanel_mid.png</texture>
        <colordiffuse>$INFO[Skin.String(diffuse_home)]</colordiffuse>
        <include>Animation_VisibleChange400</include>
        <visible>Skin.HasSetting(breadcrumbslabel)</visible>
      </control>
    </control>
    <control type="grouplist">
      <!--breadcrumbs-->
      <posx>0</posx>
      <posy>0</posy>
      <width>1920</width>
      <align>center</align>
      <aligny>center</aligny>
      <itemgap>15</itemgap>
      <visible>!IsEmpty(Container.NumItems)</visible>
      <orientation>horizontal</orientation>
      <animation effect="fade" time="400" delay="400">Visible</animation>
      <animation effect="fade" time="400">Hidden</animation>
      <control type="label">
        <textcolor>time</textcolor>
        <include>BreadcrumbsLabel</include>
        <animation effect="fade" start="100" end="0" time="500" delay="500" condition="Control.IsVisible(4051)">Conditional</animation>
        <label>$INFO[ListItem.SortLetter]   </label>
        <visible>[SubString(Container.SortMethod,$LOCALIZE[551]) | SubString(Container.SortMethod,$LOCALIZE[561]) | SubString(Container.SortMethod,$LOCALIZE[558]) | SubString(Container.SortMethod,$LOCALIZE[557]) | SubString(Container.SortMethod,$LOCALIZE[556])]</visible>
      </control>
      <control type="image">
        <description>Items Icon</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>34</width>
        <height>42</height>
        <aspectratio>keep</aspectratio>
        <texture>breadcrumbs/items.png</texture>
        <colordiffuse>bcicon</colordiffuse>
        <include>dialogeffect</include>
      </control>
      <control type="label">
        <include>BreadcrumbsLabel</include>
        <label>$INFO[Container.NumItems] $VAR[ItemLabelVar]</label>
      </control>
      <control type="image">
        <description>Items Icon</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>34</width>
        <height>42</height>
        <aspectratio>keep</aspectratio>
        <texture>breadcrumbs/pages.png</texture>
        <colordiffuse>bcicon</colordiffuse>
        <include>dialogeffect</include>
      </control>
      <control type="label">
        <include>BreadcrumbsLabel</include>
        <label>$INFO[Container.CurrentPage,$LOCALIZE[31024] ]$INFO[Container.NumPages,/] </label>
      </control>
      <control type="image">
        <description>Trailer Icon</description>
        <posx>0</posx>
        <posy>0</posy>
        <width>42</width>
        <height>42</height>
        <aspectratio>keep</aspectratio>
        <texture>common/logo_trailer.png</texture>
        <colordiffuse>bcicon</colordiffuse>
        <include>dialogeffect</include>
        <animation effect="fade" start="100" end="0" time="200" condition="IsEmpty(ListItem.Trailer)">Conditional</animation>
      </control>
    </control>
    <control type="group">
      <description>Clock + Date and Time</description>
      <animation effect="slide" start="0" end="-60" time="200" tween="quadratic" easing="out" condition="Window.IsActive(mutebug)">Conditional</animation>
      <animation effect="slide" start="0" end="60" time="200" tween="quadratic" easing="out" condition="!Skin.HasSetting(AnalogClock)">Conditional</animation>
      <control type="grouplist">
        <visible>true</visible>
        <posx>0</posx>
        <posy>0</posy>
        <width>1882</width>
        <align>right</align>
        <aligny>center</aligny>
        <orientation>horizontal</orientation>
        <control type="image">
          <description>Weather image</description>
          <posx>32</posx>
          <posy>3</posy>
          <width>38</width>
          <height>38</height>
          <aspectratio>keep</aspectratio>
          <visible>Skin.HasSetting(breadcrumbsweather)</visible>
          <texture>$INFO[Window(weather).Property(Current.ConditionIcon)]</texture>
        </control>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <height>38</height>
          <aligny>center</aligny>
          <width min="15" max="1920">auto</width>
          <font>Font_Reg17_Caps</font>
          <textcolor>bclabel</textcolor>
          <shadowcolor>bcshadow</shadowcolor>
          <scroll>false</scroll>
          <align>left</align>
          <visible>Skin.HasSetting(breadcrumbsweather)</visible>
          <label>[COLOR bclabel]$INFO[Window(Weather).Property(Current.Temperature)]$INFO[System.TemperatureUnits]                  [/COLOR]</label>
        </control>
        <control type="label">
          <posx>0</posx>
          <posy>0</posy>
          <height>38</height>
          <aligny>center</aligny>
          <width min="15" max="1920">auto</width>
          <font>Font_Reg17_Caps</font>
          <textcolor>bclabel</textcolor>
          <shadowcolor>bcshadow</shadowcolor>
          <scroll>false</scroll>
          <align>left</align>
          <label>[COLOR bclabel]$INFO[System.Date][/COLOR][COLOR=time]$INFO[System.Time,  |  ][/COLOR]       </label>
        </control>
      </control>
      <include>AnalogClock</include>
    </control>
  </include>
