          <midtexture>osd/seek/progresscache_mid.png</midtexture>
          <righttexture>osd/seek/progresscache_right.png</righttexture>
          <overlaytexture>-</overlaytexture>
          <info>Player.ProgressCache</info>
        </control>
        <control type="progress" id="20">
          <description>Progressbar</description>
          <posx>510</posx>
          <posy>0</posy>
          <width>180</width>
          <height>20</height>
          <colordiffuse>$INFO[Skin.String(diffuse_color)]</colordiffuse>
          <texturebg>osd/seek/progresscache_back.png</texturebg>
          <lefttexture>dialogs/progress/progress_left.png</lefttexture>
          <midtexture>dialogs/progress/progress_mid.png</midtexture>
          <righttexture>dialogs/progress/progress_right.png</righttexture>
          <overlaytexture>-</overlaytexture>
          <info>Player.Progress</info>
        </control>
        <control type="fadelabel">
          <posx>142</posx>
          <posy>0</posy>
          <width>352</width>
          <height>38</height>
          <aligny>center</aligny>
          <font>Font_Reg17_Caps</font>
          <textcolor>white2</textcolor>
          <scroll>true</scroll>
          <label>[COLOR a8FFFFFF]$INFO[MusicPlayer.Artist][/COLOR]</label>
          <label>[COLOR white2]$INFO[MusicPlayer.Title][/COLOR]</label>
          <scrollout>false</scrollout>
          <scrollspeed>50</scrollspeed>
          <pauseatend>10000</pauseatend>
          <visible>Player.HasAudio</visible>
        </control>
        <control type="fadelabel">
          <posx>142</posx>
          <posy>0</posy>
          <width>360</width>
          <height>38</height>
          <aligny>center</aligny>
          <font>Font_Reg17_Caps</font>
          <textcolor>white2</textcolor>
          <scroll>true</scroll>
          <label>[COLOR a8FFFFFF]$INFO[Videoplayer.TVShowTitle][/COLOR]</label>
          <label>[COLOR white2]$INFO[Videoplayer.Title][/COLOR]</label>
          <scrollout>false</scrollout>
          <scrollspeed>50</scrollspeed>
          <pauseatend>10000</pauseatend>
          <visible>!IsEmpty(Videoplayer.TVShowTitle) + Player.HasVideo</visible>
        </control>
        <control type="fadelabel">
          <posx>142</posx>
          <posy>0</posy>
          <width>360</width>
          <height>38</height>
          <aligny>center</aligny>
          <font>Font_Reg17_Caps</font>
          <textcolor>white2</textcolor>
          <scroll>true</scroll>
          <label>$INFO[Videoplayer.Title]</label>
          <scrollout>false</scrollout>
          <scrollspeed>50</scrollspeed>
          <pauseatend>10000</pauseatend>
          <visible>IsEmpty(Videoplayer.TVShowTitle) + Player.HasVideo</visible>
        </control>
        <control type="label">
          <description>Current time</description>
          <posx>450</posx>
          <posy>14</posy>
          <width>300</width>
          <height>30</height>
          <font>Font_Reg14</font>
          <label>$VAR[HomeOSDLabelVar]</label>
          <align>center</align>
          <textcolor>white2</textcolor>
        </control>
      </control>
      <control type="group">
        <visible>Skin.HasSetting(GlobalRSS) + ![Skin.HasSetting(GlobalNowPlaying) + Player.HasMedia]</visible>
        <control type="image">
          <description>rss logo</description>
          <posx>684</posx>
          <posy>4</posy>
          <width>32</width>
          <height>32</height>
          <texture>icon-rss.png</texture>
          <colordiffuse>bclabel</colordiffuse>
          <include>Animation_VisibleChange400</include>
        </control>
        <control type="rss">
          <posx>0</posx>
          <posy>4</posy>
          <width>687</width>
          <height>45</height>
          <aligny>center</aligny>
          <font>Font_Reg17_Caps</font>
          <urlset>1</urlset>
          <textcolor>88FFFFFF</textcolor>
          <headlinecolor>88FFFFFF</headlinecolor>
          <highlightcolor>88FFFFFF</highlightcolor>
          <titlecolor>dc2B60DE</titlecolor>
          <scrollspeed>50</scrollspeed>
        </control>
      </control>
    </control>
  </include>
  <include name="Furniture_NowPlayingWidget_PVR">
    <control type="group">
      <include>Animation_CommonFade</include>
      <animation type="Conditional" reversible="true" condition="Control.HasFocus(195)">
        <effect type="slide" start="0,0" end="0,-52" time="400" tween="cubic" easing="in" />
      </animation>
      <visible>Player.HasMedia</visible>
      <visible>!SubString(Window(videolibrary).Property(tvtunesIsAlive),True)</visible>
      <posx>-132</posx>
      <posy>0</posy>
      <control type="progress">
