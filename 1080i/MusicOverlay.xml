<?xml version="1.0" encoding="utf-8"?>
<window id="2004">
    <onload condition="system.hasaddon(script.artistslideshow) + [Skin.HasSetting(homeartistslideshow) | Skin.HasSetting(smallmusichome)]">RunScript(script.artistslideshow)</onload>
    <onunload>Skin.Reset(rotatenowplaying)</onunload>
    <onload>AlarmClock(NowPlaying,Skin.ToggleSetting(rotatenowplaying),00:15,silent,loop)</onload>
    <onunload>CancelAlarm(NowPlaying,true)</onunload>
    <onunload>Skin.Reset(rotatenowplaying)</onunload>
    <defaultcontrol>-</defaultcontrol>
    <controls></controls>
</window>